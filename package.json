{
  "name": "nodejs-llm-job-matcher",
  "version": "1.0.0",
  "description": "Node.js backend for LLM-powered job matching using Pinecone vector search",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop ecosystem.config.js",
    "restart:prod": "pm2 restart ecosystem.config.js",
    "dev": "nodemon index.js",
    "setup": "node setup.js",
    "populate-data": "node sampleData.js populate",
    "clear-data": "node sampleData.js clear",
    "index-stats": "node sampleData.js stats",
    "logs": "pm2 logs statethon-backend",
    "monitor": "pm2 monit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "langchain",
    "pinecone",
    "groq",
    "llm",
    "vector-search",
    "job-matching"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.12",
    "@pinecone-database/pinecone": "^2.0.1",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.7",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "langchain": "^0.1.25",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.4",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
